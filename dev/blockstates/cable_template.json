{
    "multipart": [
        {
            "apply":{ "model":"kontrol:block/wires/[WIRE]/none" }
        },
        {
            "when":{ "attach_up": "true" },
            "apply":{ "model":"kontrol:block/wires/[WIRE]/up" }
        },
        {
            "when":{ "attach_down": "true" },
            "apply":{ "model":"kontrol:block/wires/[WIRE]/up", "x": 180 }
        },
        {
            "when":{ "attach_north": "true" },
            "apply":{ "model":"kontrol:block/wires/[WIRE]/up", "x": 90, "y": 0 }
        },
        {
            "when":{ "attach_south": "true" },
            "apply":{ "model":"kontrol:block/wires/[WIRE]/up", "x": 90, "y": 180 }
        },
        {
            "when":{ "attach_west": "true" },
            "apply":{ "model":"kontrol:block/wires/[WIRE]/up", "x": 90, "y": 270}
        },
        {
            "when":{ "attach_east": "true" },
            "apply":{ "model":"kontrol:block/wires/[WIRE]/up", "x": 90, "y": 90 }
        }
    ]
}